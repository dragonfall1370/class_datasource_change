    <connection/>
    <sql>select 1111 as Client_form_id
, 9999 as Client_field_id
, row_number() over(order by Column_as_values) + 'NO.' as Client_field_value --NO. the current field value number
, 'title_language_code_' || row_number() over(order by Column_as_values) + 'MaxID' as Client_title_language_code
, 'en' as Client_language
, Column_as_values as Client_translate
, row_number() over(order by intv_selected_bywho) + 'MaxID' as Client_id --'MaxID' from configurable_form_field_value
from Column_as_values
limit 10</sql>
