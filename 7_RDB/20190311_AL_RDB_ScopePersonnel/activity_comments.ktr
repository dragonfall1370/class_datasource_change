  <step>
    <name>Job AC &lt;= Source 1</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>cusDB</connection>
    <sql>declare @NewLineChar char(1) = char(10);

select
	cast(x.JOBS_ID as varchar(10)) as external_id
	--, cast('-10' as int) as 'user_account_id'
	, lower(dbo.ufn_TrimSpecialCharacters_V2(isnull(x.USER_ID, ''), '')) as UserEmail
	, 'comment' as category
	, 'job' as type
	, dateadd(day, -2, cast(x.ENTERED as datetime)) as insert_timestamp
	, concat(
		nullif(concat('Due: ', FORMAT(dateadd(day, -2, cast(x.DUE as datetime)), 'dd-MMM-yyyy H:mm:ss', 'en-gb')), concat('Due: ', null))
		, nullif(
			concat(@NewLineChar, 'Completed: '
			, case(x.COMPLETED)
				when '1' then 'Yes'
				when '0' then 'No'
				end
			)
			, concat(@NewLineChar, 'Completed: '))
		, nullif(concat(@NewLineChar, @NewLineChar, 'Summary:', @NewLineChar, dbo.ufn_ConvertHTMLToText(trim(isnull(x.SUMMARY, '')))), concat(@NewLineChar, @NewLineChar, 'Summary:', @NewLineChar))
		, nullif(concat(@NewLineChar, @NewLineChar, 'Description:', @NewLineChar, dbo.ufn_ConvertHTMLToText(trim(isnull(x.DESCRIPTION, '')))), concat(@NewLineChar, @NewLineChar, 'Description:', @NewLineChar))
	) as content

from CALLBACKS_DATA_TABLE x
where x.JOBS_ID is not null
--and cast(x.JOBS_ID as varchar(10)) in (select [position-externalId] from VC_Job)
--and cast(x.ENTERED as datetime) > dateadd(month, -6, getdate())
order by dateadd(day, -2, cast(x.ENTERED as datetime))</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
