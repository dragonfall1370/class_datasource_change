  <step>
    <name>Job AC &lt;= Source 2</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>cusDB</connection>
    <sql>declare @NewLineChar char(1) = char(10);

select
	x.JobId as external_id
	--, cast('-10' as int) as 'user_account_id'
	, lower(dbo.ufn_TrimSpecialCharacters_V2(isnull(x.CreatedBy, ''), '')) as UserEmail
	, 'comment' as category
	, 'job' as type
	, x.CreatedOn as insert_timestamp
	, concat(
		--nullif(concat('Entered: ', FORMAT(CreatedOn, 'dd-MMM-yyyy H:mm:ss', 'en-gb')), concat('Entered: ', null))
		nullif(concat(@NewLineChar, 'Notebook Type: ', trim(isnull(x.NotebookType, ''))), concat(@NewLineChar, 'Notebook Type: '))
		, nullif(concat(@NewLineChar, 'Folder Name: ', trim(isnull(x.FolderName, ''))), concat(@NewLineChar, 'Folder Name: '))
		, nullif(concat(@NewLineChar, 'Link Type: ', trim(isnull(x.LinkType, ''))), concat(@NewLineChar, 'Link Type: '))
		, nullif(concat(@NewLineChar, 'Company ID: ', trim(coalesce(cast(x.ComId as varchar(20)), cast(x.ComObjId as varchar(20)), ''))), concat(@NewLineChar, 'Company ID: '))
		, nullif(concat(@NewLineChar, 'Contact ID: ', trim(isnull(cast(x.ConObjId as varchar(20)), ''))), concat(@NewLineChar, 'Contact ID: '))
		--, nullif(concat(@NewLineChar, 'Job ID: ', trim(isnull(cast(x.JobId as varchar(20)), ''))), concat(@NewLineChar, 'Job ID: '))
		, nullif(concat(@NewLineChar, 'Candidate ID: ', trim(isnull(cast(x.CanObjId as varchar(20)), ''))), concat(@NewLineChar, 'Candidate ID: '))
		, nullif(concat(@NewLineChar, 'Subject: ', trim(isnull(x.Subject, ''))), concat(@NewLineChar, 'Subject: '))
		, nullif(concat(@NewLineChar, 'Content:', @NewLineChar, isnull(nullif(cast(patindex('{\rtf%', trim(isnull(x.NoteContent, ''))) as varchar(max)), '0'), trim(isnull(x.NoteContent, '')))), concat(@NewLineChar, 'Content:', @NewLineChar))
	) as content

from VC_JobActivityCommentNotebook x
where x.JobId is not null
order by x.CreatedOn</sql>
