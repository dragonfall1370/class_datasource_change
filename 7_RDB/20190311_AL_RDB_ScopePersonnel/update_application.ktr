    <sql>;with 
TmpTab1 as (
	select
	cast(JobId as varchar(10)) JobExtId
	, cast(ApplicantId as varchar(10)) CanExtId
	, isnull(ShortlistedDate, coalesce(SentDate, FirstInterviewDate, SecondInterviewDate, OfferedDate, PlacedDate, null)) ShortListedDate
	, isnull(SentDate, coalesce(FirstInterviewDate, SecondInterviewDate, OfferedDate, PlacedDate, null)) SentDate
	, isnull(FirstInterviewDate, coalesce(SecondInterviewDate, OfferedDate, PlacedDate, null)) FirstInterviewDate
	, isnull(SecondInterviewDate, coalesce(OfferedDate, PlacedDate, null)) SecondInterviewDate
	, isnull(OfferedDate, coalesce(PlacedDate, null)) OfferedDate
	, PlacedDate PlacedDate
	from VC_Applications3
)

, TmpTab2 as (
select
JobExtId
, CanExtId
, Max(ShortListedDate) ShortListedDate
, Max(SentDate) SentDate
, Max(FirstInterviewDate) FirstInterviewDate
, Max(SecondInterviewDate) SecondInterviewDate
, Max(OfferedDate) OfferedDate
, Max(PlacedDate) PlacedDate
from TmpTab1
--where CanExtId = 22769 and JobExtId = 16381
group by JobExtId, CanExtId
)

select * from TmpTab2
where ShortListedDate is not null or SentDate is not null or FirstInterviewDate is not null or SecondInterviewDate is not null or OfferedDate is not null or PlacedDate is not null</sql>
