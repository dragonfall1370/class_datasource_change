    <connection>CAGlobal DB</connection>
    <sql>select concat('CG',c.id) as CAG_CandidateExtID
, concat_ws(chr(10),'[Candidate Activities]'
	, coalesce('Activity ID: '||nullif(nullif(c.actid,''),'NA'),NULL)
	, coalesce('Activity type: '||nullif(nullif(c.acttype,''),'NA'),NULL)
	, coalesce('Entered by: '|| u.user_name || u.first_name || ' ' || u.last_name)
	, coalesce('Activity notes: '||nullif(nullif(c.actnotes,''),'NA'),NULL)
	) as CAG_comment_activities
, c.actdate::timestamp as CAG_insert_timestamp
, -10 as CAG_user_account_id
, 'comment' as CAG_category
, 'candidate' as CAG_type
from candidateactivities c
left join users u on u.user_id = c.actenteredby
where c.actnotes &lt;&gt; ''
order by c.id

