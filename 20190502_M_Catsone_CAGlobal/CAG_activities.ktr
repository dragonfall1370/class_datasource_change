    <connection>CAG DB PROD</connection>
    <sql>--COMPANY ACTIVITIES
select concat('CG', a.data_item_id) as CompanyExtID
, concat_ws (char(10)
	, coalesce('Date Created: ' + nullif(convert(varchar(max),try_parse(a.date_created as datetime),120),''),NULL)
	, coalesce('Created by: ' + concat_ws(' - ', nullif(u.username,''), nullif(u.firstname,''), nullif(u.lastname,'')),NULL)
	, coalesce('Type: ' + nullif(a.Type,''),NULL)
	, coalesce('Annotation: ' + nullif(a.annotation,''),NULL)
	, coalesce('Notes: ' + char(10) + nullif(a.notes,''),NULL)
	) as comment_activities
, try_parse(a.date_created as datetime) as insert_timestamp
, -10 as user_account_id
, 'comment' as category
, 'company' as type
from activities a
left join users u on u.id = a.entered_by_id
where data_item_type = 'company'
order by a.data_item_id</sql>
