  <step>
    <name>JobApp From OFFERED Stage</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>source</connection>
    <sql>with Offers as &#x28;select j.CVSENT_ID&#xa;	, c.JOB_ID&#xa;	, r.JOB_TITLE&#xa;	, c.APP_ID&#xa;	, a.FIRST_NAME&#xa;	, a.LAST_NAME&#xa;	, 5 as Stage --OFFERED&#xa;	, &#x27;Job Offers&#x27; as SubStatus&#xa;	, j.DATE_TIME as OfferDate&#xa;	, j.DATE_TIME as StartDate&#xa;	, case when r.JOB_TYPE &#x3d; 6 then j.DATE_TIME &#x2b; 60 --PERMANENT&#xa;	when r.JOB_TYPE &#x3d; 5 then j.DATE_TIME &#x2b; 30 --CONTRACT&#xa;	when r.JOB_TYPE &#x3d; 7 then j.DATE_TIME &#x2b; 30 --TEMP&#xa;	else j.DATE_TIME &#x2b; 30 end as EndDate&#xa;	from JobOffers j&#xa;	left join CVsSent c on c.CVSENT_ID &#x3d; j.CVSENT_ID&#xa;	left join Applicants a on c.APP_ID &#x3d; a.APP_ID&#xa;	left join Requirements r on c.JOB_ID &#x3d; r.JOB_ID&#xa;	where j.CVSENT_ID is not NULL and c.JOB_ID is not NULL&#xa;&#xa;UNION ALL&#xa;&#xa;	select c.CVSENT_ID&#xa;	, c.JOB_ID&#xa;	, r.JOB_TITLE&#xa;	, c.APP_ID&#xa;	, a.FIRST_NAME&#xa;	, a.LAST_NAME&#xa;	, 5 as Stage --OFFERED&#xa;	, &#x27;Under Offer&#x27; as SubStatus&#xa;	, c.DATE_SENT as OfferDate&#xa;	, c.DATE_SENT as StartDate&#xa;	, case when r.JOB_TYPE &#x3d; 6 then c.DATE_SENT &#x2b; 60 --PERMANENT &#x7c; this rule from HiringBoss &#x5b;default open days&#x5d;&#xa;	when r.JOB_TYPE &#x3d; 5 then c.DATE_SENT &#x2b; 30 --CONTRACT&#xa;	when r.JOB_TYPE &#x3d; 7 then c.DATE_SENT &#x2b; 30 --TEMP&#xa;	else c.DATE_SENT &#x2b; 30 end as EndDate&#xa;	from CVsSent c&#xa;	left join Applicants a on c.APP_ID &#x3d; a.APP_ID&#xa;	left join Requirements r on c.JOB_ID &#x3d; r.JOB_ID&#xa;	where c.STATUS in &#x28;1&#x29; --Under Offer&#xa;	&#x29; &#xa;&#xa;, OfferStage as &#x28;select CVSENT_ID&#xa;	, JOB_ID&#xa;	, JOB_TITLE&#xa;	, APP_ID&#xa;	, FIRST_NAME&#xa;	, LAST_NAME&#xa;	, 5 as Stage --OFFERED&#xa;	, SubStatus&#xa;	, OfferDate&#xa;	, StartDate&#xa;	, EndDate&#xa;	, row_number&#x28;&#x29; over&#x28;partition by JOB_ID, APP_ID order by OfferDate desc&#x29; as Offerrn&#xa;from Offers&#x29;&#xa;&#xa;, FinalOffer as &#x28;select CVSENT_ID&#xa;	, JOB_ID&#xa;	, JOB_TITLE&#xa;	, APP_ID&#xa;	, FIRST_NAME&#xa;	, LAST_NAME&#xa;	, 5 as Stage --OFFERED&#xa;	, SubStatus&#xa;	, OfferDate --used as placced_date as well&#xa;	, StartDate&#xa;	, EndDate&#xa;	from OfferStage&#xa;	where Offerrn &#x3d; 1&#x29;&#xa;&#xa;--PLACED&#xa;, Placed as &#x28;select p.ID&#xa;	, p.JOB_ID&#xa;	, r.JOB_TITLE&#xa;	, p.APP_ID&#xa;	, a.FIRST_NAME&#xa;	, a.LAST_NAME&#xa;	, 6 as Stage --PLACED stage&#xa;	, &#x27;Placed&#x27; as SubStatus&#xa;	, p.CREATED_ON as OfferDate --used as placed_date&#xa;	, case when &#x28;p.START_DATE &#x2b; p.START_TIME&#x29; is NULL then p.CREATED_ON&#xa;	else &#x28;p.START_DATE &#x2b; p.START_TIME&#x29; end as StartDate&#xa;	, case when &#x28;p.END_DATE &#x2b; p.END_TIME&#x29; is NULL then &#xa;		&#x28;case when r.JOB_TYPE &#x3d; 6 then p.CREATED_ON &#x2b; 60 --PERMANENT &#x7c; this rule from HiringBoss &#x5b;default open days&#x5d;&#xa;		when r.JOB_TYPE &#x3d; 5 then p.CREATED_ON &#x2b; 30 --CONTRACT&#xa;		when r.JOB_TYPE &#x3d; 7 then p.CREATED_ON &#x2b; 30 --TEMP&#xa;		else p.CREATED_ON &#x2b; 30 end&#x29;&#xa;	else &#x28;p.END_DATE &#x2b; p.END_TIME&#x29; end as EndDate&#xa;from NewPlacements p--Placed stage&#xa;left join Applicants a on p.APP_ID &#x3d; a.APP_ID&#xa;left join Requirements r on p.JOB_ID &#x3d; r.JOB_ID&#x29;&#xa;&#xa;--JOIN APPLICATION&#xa;, JobApp as &#x28;&#xa;	select &#x2a; from FinalOffer&#xa;	UNION ALL&#xa;	select &#x2a; from Placed&#x29;&#xa;&#xa;, FinalJobApp as &#x28;select CVSENT_ID&#xa;	, JOB_ID&#xa;	, JOB_TITLE&#xa;	, APP_ID&#xa;	, FIRST_NAME&#xa;	, LAST_NAME&#xa;	, Stage&#xa;	, SubStatus&#xa;	, OfferDate&#xa;	, StartDate&#xa;	, EndDate&#xa;	, row_number&#x28;&#x29; over&#x28;partition by JOB_ID, APP_ID order by Stage desc, OfferDate desc&#x29; as JobApprn --OfferDate as ActionedDate&#xa;	from JobApp&#x29;&#xa;&#xa;select ja.JOB_ID as JobExtID&#xa;	, APP_ID as CandidateExtID&#xa;	, SubStatus&#xa;	, OfferDate&#xa;	, StartDate&#xa;	, EndDate&#xa;	, &#x27;&#x5b;The offer&#x2f;placement details were migrated from Apis&#x5d;&#x27; as PlacementNote&#xa;	, 3 as draft_offer --used to move OFFERED to PLACED in VC &#x5b;offer&#x5d;&#xa;	, 2 as InvoiceStatus --used to update invoice status in VC &#x5b;invoice&#x5d; as &#x27;active&#x27;&#xa;	, 1 as renewal_index --default value in VC &#x5b;invoice&#x5d;&#xa;	, 1 as renewal_flow_status&#xa;	, -10 as Latest_user_id&#xa;	, getdate&#x28;&#x29; as Latest_update_date&#xa;	, 0 as Tax_rate&#xa;	, &#x27;other&#x27; as Export_data_to&#xa;	, 0 as Net_total&#xa;	, 0 as Other_invoice_items_total&#xa;	, 0 as Invoice_total&#xa;from FinalJobApp ja&#xa;where JobApprn &#x3d; 1 --117 rows</sql>
    <limit>0</limit>
