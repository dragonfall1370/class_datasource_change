    <connection>source</connection>
    <sql>--SHORTLISTED&#xa;&#x2f;&#x2a; Status&#x3a; 15 Chase &#x7c; 11 Not Suitable &#x7c; 9 Not Interested &#x7c; 13 Called for Referrals &#x7c; 10 Competitor Contacted &#x2a;&#x2f;&#xa;with Shortlisted as &#x28;select convert&#x28;nvarchar&#x28;max&#x29;,p.PROSPECT_ID&#x29; as PROSPECT_ID&#xa;	, p.JOB_ID&#xa;	, r.JOB_TITLE&#xa;	, p.APP_ID&#xa;	, a.FIRST_NAME&#xa;	, a.LAST_NAME&#xa;	, 1 as Stage --SHORTLISTED&#xa;	, case when p.STATUS_ID &#x3d; 9 then &#x27;Not Interested&#x27;&#xa;		when p.STATUS_ID &#x3d; 11 then &#x27;Not Suitable&#x27;&#xa;		when p.STATUS_ID &#x3d; 15 then &#x27;Chase&#x27;&#xa;		when p.STATUS_ID &#x3d; 13 then &#x27;Called for Referrals&#x27;&#xa;		when p.STATUS_ID &#x3d; 10 then &#x27;Competitor Contacted&#x27;&#xa;		else NULL end as SubStatus&#xa;	, p.DATE_ADDED as ActionedDate&#xa;	from Prospects p&#xa;	left join Applicants a on p.APP_ID &#x3d; a.APP_ID&#xa;	left join Requirements r on r.JOB_ID &#x3d; p.JOB_ID&#x29;&#xa;&#xa;--SENT&#xa;&#x2f;&#x2a; Status&#x3a; Sent 0 &#x7c; Awaiting Interview 3 &#x7c; Interview 6 &#x7c; Under Offer 1 &#x7c; Placed 2 &#x7c; Rejected 4 &#x7c; Declined 5 &#x7c; Not Actioned -1 &#x2a;&#x2f;&#xa;, SentStage as &#x28;select c.CVSENT_ID&#xa;	, c.JOB_ID&#xa;	, r.JOB_TITLE&#xa;	, c.APP_ID&#xa;	, a.FIRST_NAME&#xa;	, a.LAST_NAME&#xa;	, 2 as Stage --SENT&#xa;	, case when c.STATUS &#x3d; 0 then &#x27;Sent&#x27;&#xa;		when c.STATUS &#x3d; 5 then &#x27;Declined&#x27;&#xa;		when c.STATUS &#x3d; 4 then &#x27;Rejected&#x27;&#xa;		when c.STATUS &#x3d; -1 then &#x27;Not Actioned&#x27;&#xa;		else NULL end as SubStatus&#xa;	, c.DATE_SENT as ActionedDate&#xa;	from CVsSent c&#xa;	left join Applicants a on c.APP_ID &#x3d; a.APP_ID&#xa;	left join Requirements r on c.JOB_ID &#x3d; r.JOB_ID&#xa;	where c.STATUS in &#x28;0, 4, 5, -1&#x29;&#x29;&#xa;&#xa;--INTERVIEW in CVsSent&#xa;, MapInterviews as &#x28;select c.CVSENT_ID&#xa;	, c.JOB_ID&#xa;	, r.JOB_TITLE&#xa;	, c.APP_ID&#xa;	, a.FIRST_NAME&#xa;	, a.LAST_NAME&#xa;	&#x2f;&#x2a; Interview Rounds &#xa;	case when c.INTERVIEWS &#x3e; 1 then 4 --SECOND_INTERVIEW&#xa;		when c.INTERVIEWS &#x3c;&#x3d; 1 then 3 --FIRST_INTERVIEW&#xa;		else 0 end as Stage&#xa;	&#x2a;&#x2f;&#xa;	, c.INTERVIEWS as Stage&#xa;	, case when c.STATUS &#x3d; 3 then &#x27;Awaiting Interview&#x27;&#xa;		when c.STATUS &#x3d; 6 then &#x27;Interview&#x27;&#xa;		else NULL end as SubStatus&#xa;	, c.DATE_SENT as ActionedDate&#xa;	from CVsSent c&#xa;	left join Applicants a on c.APP_ID &#x3d; a.APP_ID&#xa;	left join Requirements r on c.JOB_ID &#x3d; r.JOB_ID&#xa;	where c.STATUS in &#x28;3, 6&#x29;&#xa;&#xa;UNION ALL&#xa;&#xa;--INTERVIEW in Interviews&#xa;	select i.CVSENT_ID&#xa;	, i.JOB_ID&#xa;	, r.JOB_TITLE&#xa;	, c.APP_ID&#xa;	, a.FIRST_NAME&#xa;	, a.LAST_NAME&#xa;	, i.NUM as Stage&#xa;	, &#x27;Interview&#x27; as SubStatus&#xa;	, i.ARRANGE_ON as ActionedDate&#xa;	from Interviews i&#xa;	left join CVsSent c on c.CVSENT_ID &#x3d; i.CVSENT_ID&#xa;	left join Applicants a on c.APP_ID &#x3d; a.APP_ID&#xa;	left join Requirements r on i.JOB_ID &#x3d; r.JOB_ID&#x29;&#xa;&#xa;, InterviewStage as &#x28;select CVSENT_ID&#xa;	, JOB_ID&#xa;	, JOB_TITLE&#xa;	, APP_ID&#xa;	, FIRST_NAME&#xa;	, LAST_NAME&#xa;	, Stage&#xa;	, ROW_NUMBER&#x28;&#x29; over&#x28;partition by JOB_ID, APP_ID order by Stage desc, ActionedDate desc&#x29; as Interviewrn&#xa;	, SubStatus&#xa;	, ActionedDate&#xa;	from MapInterviews&#x29;&#xa;&#xa;, FinalInterview as &#x28;select CVSENT_ID&#xa;	, JOB_ID&#xa;	, JOB_TITLE&#xa;	, APP_ID&#xa;	, FIRST_NAME&#xa;	, LAST_NAME&#xa;	, case when Stage &#x3e; 1 then 4 --SECOND_INTERVIEW&#xa;		when Stage &#x3c;&#x3d; 1 then 3 --FIRST_INTERVIEW&#xa;		else 0 end as Stage&#xa;	, SubStatus&#xa;	, ActionedDate &#xa;	from InterviewStage&#x29;&#xa;&#xa;--OFFER&#xa;, Offers as &#x28;select j.CVSENT_ID&#xa;	, c.JOB_ID&#xa;	, r.JOB_TITLE&#xa;	, c.APP_ID&#xa;	, a.FIRST_NAME&#xa;	, a.LAST_NAME&#xa;	, 5 as Stage --OFFERED&#xa;	, &#x27;Job Offers&#x27; as SubStatus&#xa;	, j.DATE_TIME as ActionedDate&#xa;	from JobOffers j&#xa;	left join CVsSent c on c.CVSENT_ID &#x3d; j.CVSENT_ID&#xa;	left join Applicants a on c.APP_ID &#x3d; a.APP_ID&#xa;	left join Requirements r on c.JOB_ID &#x3d; r.JOB_ID&#xa;	where j.CVSENT_ID is not NULL and c.JOB_ID is not NULL&#xa;&#xa;UNION ALL&#xa;&#xa;	select c.CVSENT_ID&#xa;	, c.JOB_ID&#xa;	, r.JOB_TITLE&#xa;	, c.APP_ID&#xa;	, a.FIRST_NAME&#xa;	, a.LAST_NAME&#xa;	, 5 as Stage --OFFERED&#xa;	, &#x27;Under Offer&#x27; as SubStatus&#xa;	, c.DATE_SENT as ActionedDate&#xa;	from CVsSent c&#xa;	left join Applicants a on c.APP_ID &#x3d; a.APP_ID&#xa;	left join Requirements r on c.JOB_ID &#x3d; r.JOB_ID&#xa;	where c.STATUS in &#x28;1&#x29;&#x29; --Under Offer&#xa;&#xa;, OfferStage as &#x28;select CVSENT_ID&#xa;	, JOB_ID&#xa;	, JOB_TITLE&#xa;	, APP_ID&#xa;	, FIRST_NAME&#xa;	, LAST_NAME&#xa;	, 5 as Stage --OFFERED&#xa;	, SubStatus&#xa;	, ActionedDate as ActionedDate&#xa;	, row_number&#x28;&#x29; over&#x28;partition by JOB_ID, APP_ID order by ActionedDate desc&#x29; as Offerrn&#xa;from Offers&#x29;&#xa;&#xa;, FinalOffer as &#x28;select CVSENT_ID&#xa;	, JOB_ID&#xa;	, JOB_TITLE&#xa;	, APP_ID&#xa;	, FIRST_NAME&#xa;	, LAST_NAME&#xa;	, 5 as Stage --OFFERED&#xa;	, SubStatus&#xa;	, ActionedDate as ActionedDate&#xa;	from OfferStage&#xa;	where Offerrn &#x3d; 1&#x29;&#xa;&#xa;--PLACED&#xa;, Placed as &#x28;select p.ID&#xa;	, p.JOB_ID&#xa;	, r.JOB_TITLE&#xa;	, p.APP_ID&#xa;	, a.FIRST_NAME&#xa;	, a.LAST_NAME&#xa;	, 6 as Stage --PLACED stage&#xa;	, &#x27;Placed&#x27; as SubStatus&#xa;	, p.CREATED_ON as ActionedDate &#xa;from NewPlacements p--Placed stage&#xa;left join Applicants a on p.APP_ID &#x3d; a.APP_ID&#xa;left join Requirements r on p.JOB_ID &#x3d; r.JOB_ID&#x29;&#xa;&#xa;--JOIN APPLICATION&#xa;, JobApp as &#x28;select &#x2a; from Shortlisted&#xa;	UNION ALL&#xa;	select &#x2a; from SentStage&#xa;	UNION ALL&#xa;	select &#x2a; from FinalInterview&#xa;	UNION ALL&#xa;	select &#x2a; from FinalOffer&#xa;	UNION ALL&#xa;	select &#x2a; from Placed&#x29;&#xa;&#xa;, FinalJobApp as &#x28;select PROSPECT_ID&#xa;	, JOB_ID&#xa;	, JOB_TITLE&#xa;	, APP_ID&#xa;	, FIRST_NAME&#xa;	, LAST_NAME&#xa;	, Stage&#xa;	, SubStatus&#xa;	, ActionedDate&#xa;	, row_number&#x28;&#x29; over&#x28;partition by JOB_ID, APP_ID order by Stage desc, ActionedDate desc&#x29; as JobApprn&#xa;	from JobApp&#x29;&#xa;&#xa;select ja.JOB_ID as JobExtID&#xa;, APP_ID as CandidateExtID&#xa;, case &#xa;		when Stage &#x3d; 6 then &#x27;OFFERED&#x27; --to be updated as PLACED afterward&#xa;		when Stage &#x3d; 5 then &#x27;OFFERED&#x27;&#xa;		when Stage &#x3d; 4 then &#x27;SECOND_INTERVIEW&#x27;&#xa;		when Stage &#x3d; 3 then &#x27;FIRST_INTERVIEW&#x27;&#xa;		when Stage &#x3d; 2 then &#x27;SENT&#x27;&#xa;		when Stage &#x3d; 1 then &#x27;SHORTLISTED&#x27;&#xa;		end as Stage&#xa;, case when r.JOB_TYPE &#x3d; 6 then 301 --PERMANENT&#xa;	when r.JOB_TYPE &#x3d; 5 then 302 --CONTRACT&#xa;	when r.JOB_TYPE &#x3d; 7 then 303 --TEMP&#xa;	else NULL end as PlacedStage&#xa;, SubStatus&#xa;, ActionedDate&#xa;, 3 as draft_offer --used to move OFFERED to PLACED in VC &#x5b;offer&#x5d;&#xa;, 2 as InvoiceStatus --used to update invoice status in VC &#x5b;invoice&#x5d; as &#x27;active&#x27;&#xa;, 1 as renewal_index --default value in VC &#x5b;invoice&#x5d;&#xa;, 1 as renewal_flow_status&#xa;from FinalJobApp ja&#xa;left join Requirements r on r.JOB_ID &#x3d; ja.JOB_ID&#xa;where JobApprn &#x3d; 1&#xa;and SubStatus is not NULL --244369 rows&#xa;and ja.JOB_ID is not NULL and ja.APP_ID is not NULL&#xa;order by ja.JOB_ID, ja.APP_ID&#xa;--and Stage &#x3d; 4 --5016 rows&#xa;--and Stage &#x3d; 3 --10430 rows&#xa;--and Stage &#x3d; 2 --44418 rows&#xa;--and JOB_ID &#x3d; &#x27;HQ00001033&#x27; --checking job references&#xa;--590841 rows</sql>
