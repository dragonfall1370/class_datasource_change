    <sql>--SHORTLISTED
/* Status: 15 Chase | 11 Not Suitable | 9 Not Interested | 13 Called for Referrals | 10 Competitor Contacted */
with Shortlisted as (select convert(nvarchar(max),p.PROSPECT_ID) as PROSPECT_ID
	, p.JOB_ID
	, r.JOB_TITLE
	, p.APP_ID
	, a.FIRST_NAME
	, a.LAST_NAME
	, 1 as Stage --SHORTLISTED
	, case when p.STATUS_ID = 9 then 'Not Interested'
		when p.STATUS_ID = 11 then 'Not Suitable'
		when p.STATUS_ID = 15 then 'Chase'
		when p.STATUS_ID = 13 then 'Called for Referrals'
		when p.STATUS_ID = 10 then 'Competitor Contacted'
		else NULL end as SubStatus
	, p.DATE_ADDED as ActionedDate
	from Prospects p
	left join Applicants a on p.APP_ID = a.APP_ID
	left join Requirements r on r.JOB_ID = p.JOB_ID)

--SENT
/* Status: Sent 0 | Awaiting Interview 3 | Interview 6 | Under Offer 1 | Placed 2 | Rejected 4 | Declined 5 | Not Actioned -1 */
, SentStage as (select c.CVSENT_ID
	, c.JOB_ID
	, r.JOB_TITLE
	, c.APP_ID
	, a.FIRST_NAME
	, a.LAST_NAME
	, 2 as Stage --SENT
	, case when c.STATUS = 0 then 'Sent'
		when c.STATUS = 5 then 'Declined'
		when c.STATUS = 4 then 'Rejected'
		when c.STATUS = -1 then 'Not Actioned'
		else NULL end as SubStatus
	, c.DATE_SENT as ActionedDate
	from CVsSent c
	left join Applicants a on c.APP_ID = a.APP_ID
	left join Requirements r on c.JOB_ID = r.JOB_ID
	where c.STATUS in (0, 4, 5, -1))

--INTERVIEW in CVsSent
, MapInterviews as (select c.CVSENT_ID
	, c.JOB_ID
	, r.JOB_TITLE
	, c.APP_ID
	, a.FIRST_NAME
	, a.LAST_NAME
	/* Interview Rounds 
	case when c.INTERVIEWS > 1 then 4 --SECOND_INTERVIEW
		when c.INTERVIEWS &lt;= 1 then 3 --FIRST_INTERVIEW
		else 0 end as Stage
	*/
	, c.INTERVIEWS as Stage
	, case when c.STATUS = 3 then 'Awaiting Interview'
		when c.STATUS = 6 then 'Interview'
		else NULL end as SubStatus
	, c.DATE_SENT as ActionedDate
	from CVsSent c
	left join Applicants a on c.APP_ID = a.APP_ID
	left join Requirements r on c.JOB_ID = r.JOB_ID
	where c.STATUS in (3, 6)

UNION ALL

--INTERVIEW in Interviews
	select i.CVSENT_ID
	, i.JOB_ID
	, r.JOB_TITLE
	, c.APP_ID
	, a.FIRST_NAME
	, a.LAST_NAME
	, i.NUM as Stage
	, 'Interview' as SubStatus
	, i.ARRANGE_ON as ActionedDate
	from Interviews i
	left join CVsSent c on c.CVSENT_ID = i.CVSENT_ID
	left join Applicants a on c.APP_ID = a.APP_ID
	left join Requirements r on i.JOB_ID = r.JOB_ID)

, InterviewStage as (select CVSENT_ID
	, JOB_ID
	, JOB_TITLE
	, APP_ID
	, FIRST_NAME
	, LAST_NAME
	, Stage
	, ROW_NUMBER() over(partition by JOB_ID, APP_ID order by Stage desc, ActionedDate desc) as Interviewrn
	, SubStatus
	, ActionedDate
	from MapInterviews)

, FinalInterview as (select CVSENT_ID
	, JOB_ID
	, JOB_TITLE
	, APP_ID
	, FIRST_NAME
	, LAST_NAME
	, case when Stage > 1 then 4 --SECOND_INTERVIEW
		when Stage &lt;= 1 then 3 --FIRST_INTERVIEW
		else 0 end as Stage
	, SubStatus
	, ActionedDate 
	from InterviewStage)

--OFFER
, Offers as (select j.CVSENT_ID
	, c.JOB_ID
	, r.JOB_TITLE
	, c.APP_ID
	, a.FIRST_NAME
	, a.LAST_NAME
	, 5 as Stage --OFFERED
	, 'Job Offers' as SubStatus
	, j.DATE_TIME as ActionedDate
	from JobOffers j
	left join CVsSent c on c.CVSENT_ID = j.CVSENT_ID
	left join Applicants a on c.APP_ID = a.APP_ID
	left join Requirements r on c.JOB_ID = r.JOB_ID
	where j.CVSENT_ID is not NULL and c.JOB_ID is not NULL

UNION ALL

	select c.CVSENT_ID
	, c.JOB_ID
	, r.JOB_TITLE
	, c.APP_ID
	, a.FIRST_NAME
	, a.LAST_NAME
	, 5 as Stage --OFFERED
	, 'Under Offer' as SubStatus
	, c.DATE_SENT as ActionedDate
	from CVsSent c
	left join Applicants a on c.APP_ID = a.APP_ID
	left join Requirements r on c.JOB_ID = r.JOB_ID
	where c.STATUS in (1)) --Under Offer

, OfferStage as (select CVSENT_ID
	, JOB_ID
	, JOB_TITLE
	, APP_ID
	, FIRST_NAME
	, LAST_NAME
	, 5 as Stage --OFFERED
	, SubStatus
	, ActionedDate as ActionedDate
	, row_number() over(partition by JOB_ID, APP_ID order by ActionedDate desc) as Offerrn
from Offers)

, FinalOffer as (select CVSENT_ID
	, JOB_ID
	, JOB_TITLE
	, APP_ID
	, FIRST_NAME
	, LAST_NAME
	, 5 as Stage --OFFERED
	, SubStatus
	, ActionedDate as ActionedDate
	from OfferStage
	where Offerrn = 1)

--PLACED
, Placed as (select p.ID
	, p.JOB_ID
	, r.JOB_TITLE
	, p.APP_ID
	, a.FIRST_NAME
	, a.LAST_NAME
	, 6 as Stage --PLACED stage
	, 'Placed' as SubStatus
	, p.CREATED_ON as ActionedDate 
from NewPlacements p--Placed stage
left join Applicants a on p.APP_ID = a.APP_ID
left join Requirements r on p.JOB_ID = r.JOB_ID)

--JOIN APPLICATION
, JobApp as (select * from Shortlisted
	UNION ALL
	select * from SentStage
	UNION ALL
	select * from FinalInterview
	UNION ALL
	select * from FinalOffer
	UNION ALL
	select * from Placed)

, FinalJobApp as (select PROSPECT_ID
	, JOB_ID
	, JOB_TITLE
	, APP_ID
	, FIRST_NAME
	, LAST_NAME
	, Stage
	, SubStatus
	, ActionedDate
	, row_number() over(partition by JOB_ID, APP_ID order by Stage desc, ActionedDate desc) as JobApprn
	from JobApp)

select ja.JOB_ID as JobExtID
, APP_ID as CandidateExtID
, case 
		when Stage = 6 then 'OFFERED' --to be updated as PLACED afterward
		when Stage = 5 then 'OFFERED'
		when Stage = 4 then 'SECOND_INTERVIEW'
		when Stage = 3 then 'FIRST_INTERVIEW'
		when Stage = 2 then 'SENT'
		when Stage = 1 then 'SHORTLISTED'
		end as Stage
, case when r.JOB_TYPE = 6 then 301 --PERMANENT
	when r.JOB_TYPE = 5 then 302 --CONTRACT
	when r.JOB_TYPE = 7 then 303 --TEMP
	else NULL end as PlacedStage
, SubStatus
, ActionedDate
, 3 as draft_offer --used to move OFFERED to PLACED in VC [offer]
, 2 as InvoiceStatus --used to update invoice status in VC [invoice] as 'active'
, 1 as renewal_index --default value in VC [invoice]
, 1 as renewal_flow_status
from FinalJobApp ja
left join Requirements r on r.JOB_ID = ja.JOB_ID
where JobApprn = 1
and SubStatus is not NULL --244369 rows
and ja.JOB_ID is not NULL and ja.APP_ID is not NULL
order by ja.JOB_ID, ja.APP_ID
--and Stage = 4 --5016 rows
--and Stage = 3 --10430 rows
--and Stage = 2 --44418 rows
--and JOB_ID = 'HQ00001033' --checking job references
--590841 rows</sql>
