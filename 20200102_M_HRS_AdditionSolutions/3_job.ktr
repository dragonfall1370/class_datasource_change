    <sql>--Internal description
select [PANO ] as job_ext_id
, concat_ws(concat(char(10),char(13))
	, coalesce('【社内データメモ】' + char(10) + nullif([社内データメモ], ''), NULL) --internal data memo
	, coalesce('【【社内用項目】　（複数選択可）】' + char(10) + nullif([【社内用項目】　（複数選択可）], ''), NULL) --internal items
	, coalesce('【月額 下限】' + char(10) + nullif([月額 下限], ''), NULL) --monthly amount from
	, coalesce('【月額 上限】' + char(10) + nullif([月額 上限], ''), NULL) --monthly amount to
	, coalesce('【時給 下限】' + char(10) + nullif([時給 下限], ''), NULL) --hour salary from
	, coalesce('【時給 上限】' + char(10) + nullif([時給 上限], ''), NULL) --hour salary to
	, coalesce('【求める人物像】' + char(10) + nullif([求める人物像], ''), NULL) --Desired image of a person
	, coalesce('【ココがおすすめ！】' + char(10) + nullif([ココがおすすめ！], ''), NULL) --Here is recommended
	, coalesce('【企業が候補者に一番期待すること】' + char(10) + nullif([企業が候補者に一番期待すること], ''), NULL) --What companies most expect from candidates
	, coalesce('【その他メモ】' + char(10) + nullif(その他メモ, ''), NULL) --Other notes
	) as internal_description
from csv_job
where coalesce(nullif([社内データメモ], ''), nullif([【社内用項目】　（複数選択可）], '')
	, nullif([求める人物像], ''), nullif([ココがおすすめ！], ''), nullif([企業が候補者に一番期待すること], ''), nullif([その他メモ], '')) is not NULL</sql>
