    <connection>PRTR DB</connection>
    <sql>with JobDescription as &#x28;select vac_id&#xa;, concat_ws&#x28;char&#x28;10&#x29;&#xa;	, coalesce&#x28;&#x27;Company overview&#x3a; &#x27; &#x2b; nullif&#x28;vac_benefits,&#x27;&#x27;&#x29;,NULL&#x29;&#xa;	, coalesce&#x28;&#x27;Requirements&#x3a; &#x27; &#x2b; nullif&#x28;case when vac_req like &#x27;&#x25;null&#x25;&#x27; then NULL else convert&#x28;nvarchar&#x28;max&#x29;,vac_req&#x29; end,&#x27;&#x27;&#x29;,NULL&#x29;&#xa;	, coalesce&#x28;&#x27;&#x2a;&#x2a;&#x2a;Job Description&#x3a; &#x27; &#x2b; char&#x28;10&#x29; &#x2b; char&#x28;13&#x29; &#x2b; nullif&#x28;convert&#x28;nvarchar&#x28;max&#x29;,vac_desc_html&#x29;,&#x27;&#x27;&#x29;,NULL&#x29;&#xa;	&#x29; as publicDescription&#xa;from vacancies.Vacancies&#x29;&#xa;&#xa;select concat&#x28;&#x27;PRTR&#x27;,vac_id&#x29; as JobExtID&#xa;, publicDescription&#xa;from JobDescription&#xa;where publicDescription &#x3c;&#x3e; &#x27;&#x27;&#xa;order by vac_id</sql>
    <limit>0</limit>
