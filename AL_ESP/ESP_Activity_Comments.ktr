  <step>
    <name>Job AC &lt;= Source 2</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>cusDB</connection>
    <sql>declare @NewLineChar char(1) = char(10);
declare @DoubleNewLine char(2) = @NewLineChar + @NewLineChar

select
	x.ParentId as external_id
	--, cast('-10' as int) as 'user_account_id'
	, trim(isnull(y.Username, '')) as UserEmail
	, 'comment' as category
	, 'job' as type
	, isnull(cast(x.CreatedDate as datetime), cast(x.CreatedDate as datetime)) as insert_timestamp
	, concat(
		upper('Notes')
		, @NewLineChar
		, replicate('-', len('Notes'))
		, @NewLineChar
		, trim(@NewLineChar from concat(
				iif(len(trim(isnull(x.IsPrivate, ''))) > 0, @NewLineChar + 'Private: ' + iif(trim(isnull(x.IsPrivate, '')) = '0', 'No', 'Yes'), '')
				, iif(len(trim(isnull(x.Title, ''))) > 0, @NewLineChar + 'Title: ' + trim(isnull(x.Title, '')), '')
				, iif(len(trim(isnull(x.Body, ''))) > 0,  @NewLineChar + 'Body:' + @DoubleNewLine + trim(isnull(x.Body, '')), '')
			)
		)
	) as content

from [Note] x
left join [User] y on y.Id = x.OwnerId
