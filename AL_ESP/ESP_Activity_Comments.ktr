  <step>
    <name>Job AC &lt;= Source 1</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>cusDB</connection>
    <sql>declare @NewLineChar char(1) = char(10);

select
	x.WhatId as external_id
	--, cast('-10' as int) as 'user_account_id'
	, trim(isnull(y.Username, '')) as UserEmail
	, 'comment' as category
	, 'job' as type
	, isnull(cast(x.ActivityDateTime as datetime), cast(x.CreatedDate as datetime)) as insert_timestamp
	, concat(
		upper('Event')
		, @NewLineChar
		, replicate('-', len('Event'))
		, @NewLineChar
		, trim(@NewLineChar from concat(
			'Assigned To: ' + concat(trim(isnull(y.FirstName, '')), ' ', trim(isnull(y.LastName, '')), ' (', trim(isnull(y.Username, '')), ')')
	
			, @NewLineChar + 'Subject: ' + trim(isnull(x.Subject, ''))

			, @NewLineChar + 'Private: ' + iif(x.IsPrivate = '1', 'Yes', 'No')

			, iif(len(trim(isnull(x.Location, ''))) > 0, @NewLineChar + 'Location: ' + trim(isnull(x.Location, '')), '')

			, iif(len(trim(isnull(convert(varchar(50), cast(x.ActivityDateTime as datetime), 111), ''))) > 0,  @NewLineChar + 'Start: ' + trim(isnull(convert(varchar(50), cast(x.ActivityDateTime as datetime), 111), '')), '')

			, @NewLineChar + 'All Day Event: ' + iif(x.IsAllDayEvent = 1, 'Yes', 'No')

			, iif(len(trim(isnull(x.Type, ''))) > 0, @NewLineChar + 'Type: ' + trim(isnull(x.Type, '')), '')

			, iif(len(trim(isnull(x.ShowAs, ''))) > 0, @NewLineChar + 'Show Time As: ' + trim(isnull(x.ShowAs, '')), '')

			, iif(len(trim(isnull(x.DurationInMinutes, ''))) > 0, @NewLineChar + 'Duration In Minutes: ' + trim(isnull(x.DurationInMinutes, '')), '')

			, iif(len(trim(isnull(x.Description, ''))) > 0, @NewLineChar + 'Description: ' + @NewLineChar + trim(isnull(x.Description, '')), '')
	))) as content

from [Event] x
left join [User] y on y.Id = x.OwnerId
where WhatId is not null and WhatId &lt;&gt; '000000000000000AAA'
and WhatId in (select [position-externalId] from VCJobs) -- 1
order by isnull(cast(x.ActivityDateTime as datetime), cast(x.CreatedDate as datetime))</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
    <cluster_schema/>
