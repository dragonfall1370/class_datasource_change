    <connection>PRTR DB</connection>
    <sql>with CandidateFE as (
select c.cn_id
, c.job_category
, trim(l.listvalue) as JobFunction
, getdate() as Insert_timestamp
from candidate.Jobs c
left join (select listkey, listvalue, k_id
			from common.Lists
			where listkey = 'tblVacancyJobFunctions') as l on l.k_id = c.job_category
where c.job_category > 0)

select distinct concat('PRTR',cn_id) as CandidateExtID
, JobFunction
, Insert_timestamp
from CandidateFE
where JobFunction is not NULL
order by CandidateExtID desc</sql>
