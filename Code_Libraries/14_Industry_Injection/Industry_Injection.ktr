    <connection>DATABASE</connection>
    <sql>with CompIndustry as (select ID
, INDUSTRY_SECTORS__C
from Company

UNION ALL

select ID
, INDUSTRY_SECTORS__C
from CompanyDelta
)

select 
ID as Sirius_CompExtID
--distinct value as Sirius_CompanyIndustry
, case value
	when 'Accounting' then 'Accounting'
	when 'Advertising' then 'Advertising Arts &amp; Media'
	when 'Advertising  Media Entert' then 'Advertising Arts &amp; Media'
	when 'Advertising Media Entert' then 'Advertising Arts &amp; Media'
	when 'Advertising, Arts &amp; Media' then 'Advertising Arts &amp; Media'
	when 'Agriculture, Conservation &amp; Waste' then 'Agriculture Conservation &amp; Waste'
	when 'Banking' then 'Banking &amp; Financial Services'
	when 'Banking &amp; Financial Services' then 'Banking &amp; Financial Services'
	when 'Banking and Finance' then 'Banking &amp; Financial Services'
	when 'Call Centre &amp; Customer Service' then 'Call Centre &amp; Customer Service'
	when 'Capital Equipment' then 'Capital Equipment'
	when 'Community Services &amp; Development' then 'Community Services &amp; Development'
	when 'Consulting' then 'Consulting &amp; Strategy'
	when 'Consulting &amp; Strategy' then 'Consulting &amp; Strategy'
	when 'Design &amp; Architecture' then 'Design &amp; Architecture'
	when 'Education' then 'Education &amp; Training'
	when 'Education &amp; Training' then 'Education &amp; Training'
	when 'Education and training' then 'Education &amp; Training'
	when 'Engineering' then 'Engineering'
	when 'Events, Exhibitions &amp; Conferences' then 'Events Exhibitions &amp; Conferences'
	when 'Financial Services' then 'Banking &amp; Financial Services'
	when 'FMCG and Consumer Product' then 'FMCG Retail &amp; Consumer Products'
	when 'FMCG, Retail &amp; Consumer Products' then 'FMCG Retail &amp; Consumer Products'
	when 'Government &amp; Defence' then 'Government &amp; Defence'
	when 'Healthcare' then 'Healthcare'
	when 'Healthcare &amp; Aged Care' then 'Healthcare'
	when 'Healthcare &amp; Medical' then 'Healthcare'
	when 'Healthcare &amp; Pharmaceutic' then 'Healthcare'
	when 'Hospitality, Leisure &amp; Tourism' then 'Hospitality Leisure &amp; Tourism'
	when 'Human Resources &amp; Recruitment' then 'Human Resources &amp; Recruitment'
	when 'Information &amp; Communication Technology' then 'Information &amp; Communication Technology'
	when 'Insurance' then 'Insurance'
	when 'IT and Telecommunications' then 'Telecommunications'
	when 'Legal' then 'Legal'
	when 'Manufacturing' then 'Manufacturing'
	when 'Marketing, Agency &amp; Communications' then 'Marketing Agency &amp; Communications'
	when 'Mining &amp; Resources' then 'Mining &amp; Resources'
	when 'Mining, Resources &amp; Energy' then 'Mining &amp; Resources'
	when 'Not for Profit / Public Sector' then 'Not for Profit / Public Sector'
	when 'Not for Profit/Public Sec' then 'Not for Profit / Public Sector'
	when 'Not for Profit/Public Sector' then 'Not for Profit / Public Sector'
	when 'Other' then 'Other'
	when 'Pharmaceutical &amp; Medical' then 'Pharmaceutical &amp; Medical'
	when 'Pharmaceuticals' then 'Pharmaceutical &amp; Medical'
	when 'Property &amp; Construction' then 'Real Estate Property &amp; Construction'
	when 'Real Estate, Property &amp; Construction' then 'Real Estate Property &amp; Construction'
	when 'Science &amp; Technology' then 'Science &amp; Technology'
	when 'Sport &amp; Recreation' then 'Sports Recreation &amp; Entertainment'
