    </partitioning>
    <connection>ONSET DB</connection>
    <sql>select concat('ON',pc.CandidateContactID) as CandidateExtID
, concat('ON',pc.JobOrderID) as JobExtID
, concat_ws (char(10),'[PLACEMENT NOTE]'
	, coalesce('Candidate: ' + nullif(c.FullName,''),NULL)
	, coalesce('Job: ' + nullif(pc.JobTitle,''),NULL)
	, coalesce('Company: ' + nullif(cc.Name,''),NULL)
	, coalesce('Date Created: ' + nullif(convert(varchar(max),n.DateCreated),''),NULL)
	, coalesce('Created by: ' + nullif(n.CreatedBy,''),NULL)
	, coalesce('Note type: ' + nullif(n.Type,''),NULL)
	, coalesce('Source: ' + nullif(n.Source,''),NULL)
	, coalesce('Text: ' + nullif(n.Text,''),NULL)
	, coalesce('Placement Type: ' + nullif(pc.PlacementType,''),NULL)
	, coalesce('PermanentBaseSalary: ' + convert(varchar(max),pc.PermanentBaseSalary),NULL)
	, coalesce('PermanentSuperannuation: ' + convert(varchar(max),pc.PermanentSuperannuation),NULL)
	, coalesce('PermanentTotalRemuneration: ' + convert(varchar(max),pc.PermanentTotalRemuneration),NULL)
	, coalesce('PermanentFee: ' + convert(varchar(max),pc.PermanentFee),NULL)
	, coalesce('ContractEndDate: ' + nullif(convert(varchar(max),pc.ContractEndDate),''),NULL)
	, coalesce('ContractRateType: ' + pc.ContractRateType,NULL)
	, coalesce('ContractClientRate: ' + convert(varchar(max),pc.ContractClientRate),NULL)
	, coalesce('ContractCandidateRate: ' + convert(varchar(max),pc.ContractCandidateRate),NULL)
	, coalesce('ContractOnCosts: ' + convert(varchar(max),pc.ContractOnCosts),NULL)
	, coalesce('ContractNetMargin: ' + convert(varchar(max),pc.ContractNetMargin),NULL)
	, coalesce('BillingContactID: ' + convert(varchar(max),pc.BillingContactID),NULL)
	, coalesce('Billing Contact Name: ' + nullif(c2.FullName,''),NULL)
	, coalesce('BillingOrderReference: ' + pc.BillingOrderReference,NULL)
	, coalesce('BillingInvoiceReference: ' + convert(varchar(max),pc.BillingInvoiceReference),NULL)
	, coalesce('BillingPaymentTerms: ' + pc.BillingPaymentTerms,NULL)
	, coalesce('BillingDueDate: ' + convert(varchar(max),pc.BillingDueDate),NULL)
	, coalesce('ContractCandidateRatePerWeek: ' + convert(varchar(max),pc.ContractCandidateRatePerWeek),NULL)
	, coalesce('ContractClientRatePerWeek: ' + convert(varchar(max),pc.ContractClientRatePerWeek),NULL)
	, coalesce('ContractNetMarginPerWeek: ' + convert(varchar(max),pc.ContractNetMarginPerWeek),NULL)
	, coalesce('ContractWeekCount: ' + convert(varchar(max),pc.ContractWeekCount),NULL)
	, coalesce('ContractClientRateTotal: ' + convert(varchar(max),pc.ContractClientRateTotal),NULL)
	, coalesce('ContractCandidateRateTotal: ' + convert(varchar(max),pc.ContractCandidateRateTotal),NULL)
	, coalesce('ContractNetMarginTotal: ' + convert(varchar(max),pc.ContractNetMarginTotal),NULL)
	, coalesce('ContractOnCostsType: ' + convert(varchar(max),pc.ContractOnCostsType),NULL)
	) as ON_comment_activities
, n.DateCreated as ON_insert_timestamp
, -10 as ON_user_account_id
, 'comment' as ON_category
, 'candidate' as ON_type
from PlacementNote p
left join Note n on p.NoteID = n.NoteID
left join Placement pc on pc.PlacementID = p.PlacementID
left join Contact c on c.ContactID = pc.CandidateContactID
left join Contact c2 on c2.ContactID = pc.BillingContactID
left join Company cc on cc.CompanyID = pc.CompanyContactID</sql>
