    <connection>AFR DB</connection>
    <sql>select concat('AFR',p.Person_Number) as AFR_CandidateExtID
, concat_ws(char(10),'[INTERNAL INTERVIEW]'
	, iif(a1.InternalInterviewer = '' or a1.InternalInterviewer is NULL,'No internal interviewer',concat('Internal Interviewer: ', a1.InternalInterviewer))
	, iif(p.Recruiter_Interview_DT is NULL,NULL,concat('Internal Interview Date: ', convert(varchar(20),p.Recruiter_Interview_DT,120)))
	) as AFR_comment_activities
, -10 as AFR_user_account_id
, case when Recruiter_Interview_DT is NULL then GETDATE()
	else convert(datetime,Recruiter_Interview_DT,120) end as AFR_insert_timestamp
, 'comment' as AFR_category
, 'candidate' as AFR_type
from Person p
left join AFR_CandidateCSV1 a1 on a1.CandidateNumber = p.Person_Number
where p.Recruiter_Interview_DT &lt;&gt; '' or p.Recruiter_Interview_DT is not NULL
order by p.Person_Number

