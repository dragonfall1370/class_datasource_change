  <step>
    <name>Application related comments</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>AFR DB</connection>
    <sql>select concat('AFR',p.CandidateNumber) as AFR_CandidateExtID
	, concat('AFR',p.JobNumber) as AFR_JobExtID
	, concat_ws(char(10),'[Application related comments]'
		, coalesce('Match Number: ' + convert(nvarchar(max),p.MatchNumber),'')
		, coalesce('Comment Date: ' + convert(nvarchar(20),p.comment_datetime,120),'')
		, coalesce('Candidate Number: ' + convert(varchar(10),p.CandidateNumber),NULL)
		, coalesce('Candidate Name: ' + coalesce(pp.Name_First,'') + ' ' + coalesce(pp.Name_Middle,'') + ' ' + coalesce(pp.Name_Last,''),NULL)
		, coalesce('Job Order Number: ' + convert(varchar(10),p.JobNumber),NULL)
		, coalesce('Job Title: ' + nullif(j.Position_Title,''),NULL)
		, coalesce('Comments: ' + nullif(p.Comments,''),NULL)
		) as AFR_comment_activities
	, -10 as AFR_user_account_id
	, convert(datetime,p.comment_datetime,120) as AFR_insert_timestamp
	, 'comment' as AFR_category
	, case when p.CandidateNumber > 0 then 'candidate'
	else 'job' end as AFR_type
from AFR_ApplicationCSV5 p
left join Person pp on pp.Person_Number = p.CandidateNumber
left join Job_Order j on j.Job_Order_Number = p.JobNumber
order by p.CandidateNumber, p.JobNumber

--total: 80465</sql>
