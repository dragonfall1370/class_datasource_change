    <connection>AFR DB</connection>
    <sql>select p.CandidateNumber&#xa;	, concat&#x28;&#x27;AFR&#x27;,p.JobNumber&#x29; as AFR_JobExtID&#xa;	, concat_ws&#x28;char&#x28;10&#x29;,&#x27;&#x5b;Application related comments&#x5d;&#x27;&#xa;		, coalesce&#x28;&#x27;Match Number&#x3a; &#x27; &#x2b; convert&#x28;nvarchar&#x28;max&#x29;,p.MatchNumber&#x29;,&#x27;&#x27;&#x29;&#xa;		, coalesce&#x28;&#x27;Comment Date&#x3a; &#x27; &#x2b; convert&#x28;nvarchar&#x28;20&#x29;,p.comment_datetime,120&#x29;,&#x27;&#x27;&#x29;&#xa;		, coalesce&#x28;&#x27;Candidate Number&#x3a; &#x27; &#x2b; convert&#x28;varchar&#x28;10&#x29;,p.CandidateNumber&#x29;,NULL&#x29;&#xa;		, coalesce&#x28;&#x27;Candidate Name&#x3a; &#x27; &#x2b; coalesce&#x28;pp.Name_First,&#x27;&#x27;&#x29; &#x2b; &#x27; &#x27; &#x2b; coalesce&#x28;pp.Name_Middle,&#x27;&#x27;&#x29; &#x2b; &#x27; &#x27; &#x2b; coalesce&#x28;pp.Name_Last,&#x27;&#x27;&#x29;,NULL&#x29;&#xa;		, coalesce&#x28;&#x27;Job Order Number&#x3a; &#x27; &#x2b; convert&#x28;varchar&#x28;10&#x29;,p.JobNumber&#x29;,NULL&#x29;&#xa;		, coalesce&#x28;&#x27;Job Title&#x3a; &#x27; &#x2b; nullif&#x28;j.Position_Title,&#x27;&#x27;&#x29;,NULL&#x29;&#xa;		&#x29; as AFR_comment_activities&#xa;	, -10 as AFR_user_account_id&#xa;	, p.comment_datetime as AFR_insert_timestamp&#xa;	, &#x27;comment&#x27; as AFR_category&#xa;	, &#x27;job&#x27; as AFR_type&#xa;from AFR_ApplicationCSV5 p&#xa;left join Person pp on pp.Person_Number &#x3d; p.CandidateNumber&#xa;left join Job_Order j on j.Job_Order_Number &#x3d; p.JobNumber&#xa;where p.JobNumber &#x3e; 0&#xa;order by p.JobNumber&#xa;&#xa;--80463 rows</sql>
