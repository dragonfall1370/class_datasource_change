       left join bullhorn1.BH_UserContact UC ON Cl.userID = UC.userid
       left join mail3 on mail3.ID = Cl.userid 
       where Cl.isdeleted &lt;&gt; 1  and uc.massmailoptout = 1 and mail3.ID is not null
)

