  <step>
    <name>COM &lt;= WebsiteAddress</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>YorkStreet DMPFRA</connection>
    <sql>select
	cd.CompanyID as externalId
	, iif(charindex('facebook.com', wa.URL) > 0, wa.URL, null) as url_facebook
	, iif(charindex(
		'linkedin.com', wa.URL) > 0
		, iif(
			charindex('?', wa.URL) > 0
			, substring(wa.URL, 0, charindex('?', wa.URL))
			, wa.URL)
		, null) as url_linkedin
	, iif(charindex('indeed.com', wa.URL) > 0, wa.URL, null) as url_carrier_site
from
	WebsiteAddresses wa
	join RecordTypes rt on wa.RecordTypeID = rt.RecordTypeID
	join CompanyDetails cd on wa.RecordID = cd.CompanyID
where rt.Description like
'%Companies%'
--'%Contacts%'
--'%Candidates%'</sql>
