    <connection>YorkStreet DMPFRA</connection>
    <sql>select
	CompanyID as externalId
	, iif(NoOfEmployees is not null and NoOfEmployees &lt;&gt; 0, NoOfEmployees, null)
		as employees_number
	, iif(RegistrationNumber is not null and len(RegistrationNumber) > 0, RegistrationNumber, null)
		as company_number
from
	CompanyDetails
where
	(NoOfEmployees is not null and NoOfEmployees &lt;&gt; 0)
	or (RegistrationNumber is not null and len(RegistrationNumber) > 0)</sql>
